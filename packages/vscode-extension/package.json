{
  "name": "@baseline-insight/vscode-extension",
  "displayName": "Baseline Insight",
  "version": "0.1.0",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.76.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "js"
        ],
        "extensions": [
          ".js"
        ]
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "ts"
        ],
        "extensions": [
          ".ts"
        ]
      },
      {
        "id": "html",
        "aliases": [
          "HTML",
          "htm"
        ],
        "extensions": [
          ".html",
          ".htm"
        ]
      },
      {
        "id": "css",
        "aliases": [
          "CSS"
        ],
        "extensions": [
          ".css"
        ]
      }
    ]
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:html",
    "onLanguage:css"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@baseline-insight/core-engine": "*",
    "parse5": "^8.0.0",
    "postcss": "^8.5.6",
    "vscode-languageclient": "^8.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "typescript": "^5.0.0"
  }
}
